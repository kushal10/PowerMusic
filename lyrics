cd ~/Documents/lyrics/
name=""
for var in "$@"
do
if [ "$var" == "song" ]; then
 continue
fi
if [ "$var" == "play" ]; then
 continue
fi
name="$name$var"
name="$name+"
done

python lyrics.py $name
if [ "$1" == "song" ]; then
 if [ "$2" == "play" ]; then
   echo "Playing Song"
   python download_song.py $name play  
echo "Downloading Song ......"
 else
    python download_song.py $name
 fi
fi
